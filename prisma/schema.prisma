generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Log {
  id           String   @id @default(uuid())
  sample       Int
  i            Float
  timestamp    DateTime
  identifier   String
  indicator_id Int

  @@map("logs")
}

model Indicator {
  id        Int      @id @default(autoincrement())
  i         Float
  motor     String
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("indicators")
}
